import java.util.*;
import java.io.*;

public class dowhile {
	public static void main(String args[]){
		
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("Enter a number");
		
		int value = scanner.nextInt();
		
		while(value != 5 ){
			System.out.println("Enter a number: ");
			int = scanner.nextInt();
			
		}
		System.out.println("Got 5!");
		
		
		
		
	}

}
